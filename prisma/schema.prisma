generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model User{
    id String @id @default(cuid())
    createdAt DateTime @default(now())
    email String @unique
    password String 
}

model Course {
    id String @id @default(cuid())
    createdAt DateTime @default(now())
    Email String @unique
    Password String
}

